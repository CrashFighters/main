{
    "env": {
        "node": true,
        "browser": true
    },
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "indent": [
            "warn",
            4,
            {
                "SwitchCase": 1
            }
        ],
        "no-undef": "error",
        "no-const-assign": "error",
        "no-this-before-super": "error",
        "constructor-super": "error",
        "no-eval": "error",
        "no-var": "warn",
        "no-unused-vars": "warn",
        "no-unreachable": "warn",
        "eqeqeq": "warn",
        "quotes": [
            "warn",
            "single",
            {
                "avoidEscape": true
            }
        ]
    },
    "globals": {
        "WeakMap": true,
        "Promise": true,
        "BigInt": true,
        "Symbol": true,
        "Set": true,
        "WeakSet": true,
        "Proxy": true,
        "Reflect": true,
        "grecaptcha": true,
        "Toastify": true
    }
}