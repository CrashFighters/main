{
    "env": {
        "node": true,
        "browser": true
    },
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-undef": "error",
        "no-const-assign": "error",
        "no-this-before-super": "error",
        "constructor-super": "error",
        "no-cond-assign": "error",
        "no-eval": "error",
        "indent": [
            "warn",
            4,
            {
                "SwitchCase": 1
            }
        ],
        "no-return-assign": "warn",
        "operator-assignment": "warn",
        "require-await": "warn",
        "no-var": "warn",
        "no-unused-vars": "warn",
        "no-unreachable": "warn",
        "eqeqeq": "warn",
        "prefer-const": "warn",
        "arrow-parens": "warn",
        "comma-dangle": [
            "warn",
            "never"
        ],
        "quotes": [
            "warn",
            "single",
            {
                "avoidEscape": true
            }
        ]
    },
    "globals": {
        "WeakMap": true,
        "Promise": true,
        "BigInt": true,
        "Symbol": true,
        "Set": true,
        "WeakSet": true,
        "Proxy": true,
        "Reflect": true,
        "grecaptcha": true,
        "google": true,
        "Uint8Array": true
    }
}